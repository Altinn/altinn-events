{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    },
    "logLevel": {
      "default": "Information",
      "Azure.Messaging.ServiceBus": "Warning",
      "Azure.Core": "Warning",
      "Host.Results": "Error",
      "Function": "Debug",
      "Altinn.Platform.Events.Functions.Services.PushEventsService": "Information",
      "Host.Aggregator": "Trace"
    }
  },
  "extensions": {
    "serviceBus": {
      "maxMessageBatchSize": 10,
      "autoCompleteMessages": true
    }
  }
}