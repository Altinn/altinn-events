{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    },
    "logLevel": {
      "default": "Information",
      "Function": "Debug",
      "Altinn.Platform.Events.Functions.Services.PushEventsService": "Information"
    }
  },
  "extensions": {
    "serviceBus": {
      "maxMessageBatchSize": 10,
      "autoCompleteMessages": true
    }
  }
}
